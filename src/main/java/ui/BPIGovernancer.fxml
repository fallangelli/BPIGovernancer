<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="vboxBase" prefHeight="654.0" prefWidth="534.0" styleClass="theme"
      xmlns="http://javafx.com/javafx/8" fx:controller="ui.Controller">
  <children>

    <HBox id="HBox" alignment="CENTER_LEFT" spacing="15.0">
      <children>
        <Button id="button0" fx:id="btnCheckCertNo" onAction="#CheckCertNoFired" text="无效证件号提取" textAlignment="CENTER">
        </Button>
        <Button id="button1" fx:id="btnMerge" onAction="#btnMergeFired" text="规范化姓名+重名合并">
        </Button>
        <Button id="button2" fx:id="btnExtractResultCode" onAction="#btnExtractResultFired" text="提取匹配结果">
        </Button>
        <Button id="button3" fx:id="btnCheckRegionCode" onAction="#btnCheckRegionCodeFired" text="区划代码在线验证">
        </Button>
      </children>
      <padding>
        <Insets top="20.0"/>
      </padding>
    </HBox>
    <HBox prefHeight="31.0" prefWidth="534.0">
      <children>
        <Label fx:id="labInfo" prefHeight="52.0" prefWidth="328.0" wrapText="true">
          <HBox.margin>
            <Insets/>
          </HBox.margin>
        </Label>
        <Label prefHeight="52.0" prefWidth="103.0" text="运行状态:"/>
        <Label fx:id="labRunning" prefHeight="52.0" prefWidth="139.0"/>
      </children>
    </HBox>
    <ScrollPane fx:id="scrollPane" prefHeight="561.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
      <content>
        <VBox fx:id="vBoxItems" prefHeight="521.0" prefWidth="580.0"/>
      </content>
    </ScrollPane>
  </children>
</VBox>
